<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="frontpro.Pages.Register"
             Title="Register">
    <VerticalStackLayout Padding="20">
        <Entry Placeholder="Name" x:Name="NameEntry"/>
        <Entry Placeholder="Email " x:Name="EmailEntry" Keyboard="Email"/>
        <Entry Placeholder="Paasword" x:Name="Password" IsPassword="True"/>
        <Button Text="Register " Clicked="redisterbutton_clicked" />

    </VerticalStackLayout>
</ContentPage>